	<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".StartActivity" >

    <!--
         The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc.
    -->


    <!--
         This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows.
    -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true" >

        <LinearLayout
            android:id="@+id/fullscreen_content_controls"
            style="?buttonBarStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:background="@color/black_overlay"
            android:orientation="horizontal"
            tools:ignore="UselessParent" >
        </LinearLayout>



        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/fullscreen_content_controls"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="125dp"
            android:text="@string/start_instruction"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/black_overlay" />

        <Button
            android:id="@+id/cube2_button"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignLeft="@+id/textView1"
            android:layout_below="@+id/textView1"
            android:layout_marginLeft="58dp"
            android:layout_marginTop="43dp"
            android:background="@drawable/cube2backimg"
            android:onClick="startCube2"
        />

        <Button
            android:id="@+id/button1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignLeft="@+id/cube3_button"
            android:layout_alignTop="@+id/cube2_button"
            android:background="@drawable/backbackimg"
         />

        <Button
            android:id="@+id/button2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignLeft="@+id/cube2_button"
            android:layout_alignTop="@+id/cube3_button"
            android:background="@drawable/notbackimg"
         />

        <Button
            android:id="@+id/cube3_button"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_below="@+id/button1"
            android:layout_marginRight="19dp"
            android:layout_marginTop="18dp"
            android:layout_toLeftOf="@+id/button2"
            android:background="@drawable/cube3backimg"
            android:onClick="startCube3" />

    </RelativeLayout>

</FrameLayout>
